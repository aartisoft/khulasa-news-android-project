        <param name="android-package" value="com.ionicframework.cordova.webview.IonicWebView" />
    </feature>
    <name>Khulasa news</name>
    <description>Jan jan ki awaaz</description>
    <author email="gyan.p@morrisdigital.mobi" href="http://morrisdigital.mobi">Morris digital</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="src/assets/logo/logo_icon_sm.png" />
    <splash density="hdpi" src="resources/splash/ss.png" />
    <splash density="ldpi" src="resources/splash/ss200.320.png" />
    <splash density="mdpi" src="resources/splash/ss320.480.png" />
    <splash density="xhdpi" src="resources/splash/ss480.800.png" />
