        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <feature name="IonicDeeplinkPlugin">
        <param name="android-package" value="io.ionic.links.IonicDeeplink" />
        <param name="onload" value="true" />
    </feature>
